{
	"compilerOptions": {
		"diagnostics": true,
		"module": "es6",
		"target": "es5",
		"lib": ["dom", "es2015", "es2016", "es2017"],
		"declaration": true,
		"strict": true,
		"removeComments": false,
		"newLine": "LF",
		"outDir": "temp/",
		"declarationDir": "dist/"
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}
